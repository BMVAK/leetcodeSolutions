class Solution {
    public boolean isPalindrome(int num) {
        
       int rev =0;
       int originalNum = num;
       int x;
       while(num>0){
       x= num%10;
       rev = rev *10 + x;
       num = num/10;
           
       }
       if(originalNum == rev){
       return true;
       }
       else{
            return false;
       }
    }
}
